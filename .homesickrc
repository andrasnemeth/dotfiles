`git clone --recursive  https://github.com/Eriner/zim.git "${$HOME}/.zim"`
`setopt EXTENDED_GLOB`
`git clone --recursive https://github.com/syl20bnr/spacemacs ~/.emacs.d`
`git clone --depth 1 https://github.com/junegunn/fzf.git ~/.fzf`
`~/.fzf/install`
`pacman -S powerline powerline-fonts`
`fc-cache -fv`
`ln -s "$(pip show powerline-status | grep "Location:" | cut -d ' ' -f 2)/powerline/dist/systemd/powerline-daemon.service" "${HOME}/.config/systemd/user/"`
`systemctl enable powerline-daemon`
`systemctl start powerline-daemon`
