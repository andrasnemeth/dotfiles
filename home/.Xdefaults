!!!Config Urxvt!!!
!-----------------------------------------------------
!URxvt.transparent: true
!URxvt.shading: 110
!*background:   #151515
URxvt.depth: 32
!URxvt.background: rgba:1500/1500/1500/f0ff
!*foreground:   #d0d0d0
!URxvt.font: xft:terminus:pixelsize=6:antialias=false
!urxvt*font: xft:Terminus:pixelsize=12,xft:Inconsolata\ for\ Powerline:pixelsize=12
URxvt.geometry: 100x40
URxvt.scrollBar: true
Urxvt.secondaryScroll: true
URxvt.scrollBar_right: true
URxvt.scrollBar_floating: true
URxvt.scrollstyle: plain
URxvt.scrollColor: #252525
!#404040
URxvt.cursorColor: #f03669
URxvt.saveLines: 10000
URxvt*urgentOnBell: true
!URxvt*visualBell: true
URxvt*perl-lib: /usr/lib/urxvt/perl
!!!URxvt*perl-ext-common: default,font-size,matcher,vtwheel,selection-to-clipvoard
!URxvt*perl-ext: default,font-size,matcher,vtwheel,clipboard
URxvt*perl-ext-common: default,font-size,osc-xterm-clipboard
URxvt*perl-ext: default,url-select,clipboard,matcher,font-size
URxvt*matcher.button:   1
URxvt*urlLauncher: /usr/bin/firefox
!!!URxvt*url-select.underline: true
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt*loginShell: true
URxvt.iconFile: /usr/share/icons/Clarity/scalable/apps/terminal.svg
URxvt*buffered:false
!,tabbed,font-size
!------------------------------------------------------

! :Bind C-0, C-+ and C-= to activate small, medium, and big font size resp.
! does not work :(:(:(
URxvt.keysym.C-0: command:\033]710;-*-terminesspowerline-medium-r-normal--12-*-*-*-*-*-*-*\007
URxvt.keysym.C-minus: command:\033]710;-*-terminesspowerline-medium-r-normal--18-*-*-*-*-*-*-*\007
URxvt.keysym.C-equal: command:\033]710;-*-terminesspowerline-medium-r-normal--24-*-*-*-*-*-*-*\007

! this needs to be installed from AUR the urxvt-font-size-git. also enable in perl-ext-common
URxvt.keysym.C-Up: perl:font-size:increase
URxvt.keysym.C-Down: perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal

! url-picker
URxvt.keysym.C-u:       perl:url-picker


